
  <div class="group">
    <%= f.label :Name, t("activerecord.attributes.link.name", :default => "Name"), :class => :label %>
    <%= f.text_field :name, :class => 'text_field' %>
    <span class="description">Text that will be shown as the link</span>
  </div>

  <div class="group">
    <%= f.label :Url, t("activerecord.attributes.link.url", :default => "URL"), :class => :label %>
    <%= f.text_field :url, :class => 'text_field' %>
    <span class="description">URL to this site</span>
  </div>

      
<div class="group navform wat-cf">
  <button class="button" type="submit">
    <%= image_tag("web-app-theme/tick.png", :alt => "#{t("web-app-theme.save", :default => "Save")}") %> <%= t("web-app-theme.save", :default => "Save") %>
  </button>  
  <%= link_to "#{image_tag("web-app-theme/cross.png", :alt => "#{t("web-app-theme.cancel", :default => "Cancel")}")} #{t("web-app-theme.cancel", :default => "Cancel")}", admin_links_path, :class => "button" %> 
</div>